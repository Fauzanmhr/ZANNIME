{
  "version": 2,
  "name": "zannime",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "files": [
    "index.js",
    "package.json",
    "package-lock.json",
    "public",
    "views",
    "routes",
    "controllers",
    "services",
  ]
}
